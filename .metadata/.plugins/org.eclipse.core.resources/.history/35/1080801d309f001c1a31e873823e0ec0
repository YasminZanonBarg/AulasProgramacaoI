package Animais;

public class Gato {

	// Atributos
	public String nome;
	public String cor;
	public String raca;

	// Construtor Padrão
	public Gato() {
	}

	// Métodos
	public void miar() {
		System.out.println("Miau!");
	}

	public void arranhar() {
		System.out.println("Gato está arranhando");
	}

	public void correr() {
		System.out.println("Correndo");
	}

}
